package Javaprogram;

public class FindSumOfNum {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		 int[] numbers = {5, 10, 15, 20};  // Example numbers
	        int sum = 0;

	        for(int i = 0; i < numbers.length; i++) {
	            sum = sum + numbers[i];
	        }

	        System.out.println("Sum of numbers = " + sum);
	    }
	}

	


