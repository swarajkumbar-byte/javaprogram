package Javaprogram ;

class Sum2Darray {
    public static void main(String[] args) {
        int arr[][] = {
            {2, 3, 4},
            {1, 5, 2},
            {2, 4, 5}
        };

        int sum = 0;

        for (int i = 0; i < arr.length; i++) {          
            for (int j = 0; j < arr[i].length; j++) {   
                sum = sum + arr[i][j];
            }
        }

        System.out.println("Sum = " + sum);
    }
}
